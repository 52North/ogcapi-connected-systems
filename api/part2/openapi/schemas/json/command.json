{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "description": "Local ID of the command",
      "type": "string",
      "minLength": 1
    },
    "control@id": {
      "description": "Local ID of the control stream where the command is part of",
      "type": "string",
      "minLength": 1
    },
    "issueTime": {
      "description": "Time at which the command was issued",
      "$ref": "../common/commonDefs.json#/definitions/TimeInstant"
    },
    "executionTime": {
      "description": "Time period during which the command was executed",
      "$ref": "../common/commonDefs.json#/definitions/TimePeriod"
    },
    "userId": {
      "description": "ID of the user who submitted the command",
      "type": "string",
      "minLength": 1
    },
    "currentStatus": {
      "description": "Current status of the command",
      "$ref": "commandStatusCode.json"
    },
    "params": {
      "description": "Command parameters"
    }
  },
  "required": [
    "params"
  ]
}